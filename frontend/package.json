{
  "name": "money-management-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:unit": "vitest --run",
    "test:unit:watch": "vitest --watch",
    "test:unit:coverage": "vitest --coverage --run",
    "test:unit:ui": "vitest --ui",
    "test:all": "npm run test:unit && npm run test:e2e",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:smoke": "playwright test --grep=\"smoke\"",
    "test:e2e:regression": "playwright test --grep=\"regression\"",
    "test:e2e:integration": "playwright test --grep=\"integration\"",
    "test:e2e:visual": "VISUAL_REGRESSION=true playwright test --grep=\"visual\"",
    "test:e2e:performance": "playwright test --grep=\"performance\"",
    "test:e2e:parallel": "playwright test --workers=4",
    "test:e2e:fast": "playwright test --grep=\"smoke|regression\" --workers=4",
    "test:e2e:report": "playwright show-report playwright-report"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.16",
    "jsdom": "^26.1.0",
    "msw": "^2.10.5",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vitest": "^3.2.4"
  }
}
